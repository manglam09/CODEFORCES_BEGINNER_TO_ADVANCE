



#include <iostream>
using namespace std;

int main() {
    string num1, num2;   //we are using string because int has size limit up to 100 for binary digits -> so,not good approach of using while loop >...
    cin >> num1 >> num2;

    string ans = "";

    for (int i = 0; i < num1.length(); i++) {
        if (num1[i] != num2[i])
            ans += '1';
        else
            ans += '0';
    }

    cout << ans << endl;
    return 0;
}
///this one is wroong
