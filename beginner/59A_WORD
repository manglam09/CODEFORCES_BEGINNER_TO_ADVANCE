// Ouestion link:-
https://codeforces.com/problemset/problem/59/A

//answer
#include <iostream>
#include<string>
#include<vector>
#include<algorithm>
using namespace std;

string CountCase(string &s){
    int lower=0,upper=0;
    for(char c:s){
        if(isupper(c)) upper++;
        else lower++;
    }
    if (lower<upper){
        transform(s.begin(), s.end(), s.begin(), ::toupper);
    }else{
        transform(s.begin(),s.end(),s.begin(), ::tolower);
    }
    return s;
    
}

int main() {
    string s;
    cin>>s;
     CountCase(s);
     cout<<s<<endl;
    return 0;
}
